{{#each this}}
{{#unless watched}}
  <div class='col-xs-3 row-centered movie'>
    <a href='#'>
      <div class='thumbnail'>
        <div class="wrapper"><span class="glyphicon glyphicon-trash delete" aria-hidden="true"></span></div>
        <img src='{{Poster}}' alt='{{Title}}'>
         <button type="button" class="btn btn-primary watched {{watched}}">Watched?</button>
        </div>
    </a>
  </div>
{{/unless}}
{{/each}}